{
  {{ if .ID }}
    "id": {{ .ID }},
  {{ end }}

  {{ if .Username }}
    "username": "{{ .Username }}",
  {{ end }}

  {{ if .Phone }}
    "phone": "{{ .Phone }}",
  {{ end }}

  {{ if .Email }}
    "email": "{{ .Email }}",
  {{ end }}

  {{ if .Additional }}
    "additional": {
        {{ range $key, $value := .Additional }}
           "{{ $key }}": {{ $value }},
        {{end}}
    }
  {{ end }}
}