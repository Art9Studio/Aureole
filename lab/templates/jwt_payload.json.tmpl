{
  {{ if .ID }}
    "id": {{ .ID }},
  {{ end }}
  {{ if .SocialID }}
    "social_id": {{ .SocialID }},
  {{ end }}
  {{ if .Username }}
    "username": "{{ .Username }}",
  {{ end }}
  {{ if .Phone }}
    "phone": "{{ .Phone }}",
  {{ end }}
  {{ if .Email }}
    "email": "{{ .Email }}",
  {{ end }}
  {{ if .UserData }}
    "user_data": {{ .UserData }},
  {{ end }}
  {{ if .Additional }}
    "additional": {
        {{ range $key, $value := .Additional }}
           "{{ $key }}": {{ $value }},
        {{end}}
    },
  {{ end }}
}