{
  {{ if .ID }}
    "id": {{ .ID }},
  {{ end }}

  {{ if .Username }}
    "username": "{{ .Username }}",
  {{ end }}

  {{ if .Phone }}
    "phone": "{{ .Phone }}",
  {{ end }}

  {{ if .Email }}
    "email": "{{ .Email }}",
  {{ end }}

  {{ if .Additional }}
    "additional": {{ json .Additional }}
  {{ end }}
}